{
    "name": "flaphl/injection",
    "description": "Dependency injection container abstractions pulled from Flaphl elements.",
    "type": "library",
    "license": "MIT",
    "keywords": ["dependency-injection", "di", "container", "service-locator", "flaphl", "php"],
    "authors": [
        {
            "name": "Jade Phyressi",
            "email": "jade@flaphl.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "psr/container": "^2.0",
        "flaphl/deprecation-contracts": "^2.1",
        "symfony/yaml": "^6.0 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": { "Flaphl\\Element\\Injection\\": "" },
        "exclude-from-classmap": ["/Tests/"]
    },
    "autoload-dev": {
        "psr-4": { "Flaphl\\Element\\Injection\\Tests\\": "Tests/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}